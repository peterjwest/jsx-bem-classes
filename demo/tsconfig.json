{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "sourceMap": false,
    "module": "commonjs",
    "target": "es5",
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
  },
  "include": [
    "*.ts"
  ]
}
